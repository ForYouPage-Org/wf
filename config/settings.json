{
  "organization": "ForYouPage-Org",
  "settings": {
    "issue_limits": {
      "max_open_issues_per_repo": 50,
      "max_sprint_current": 10,
      "max_in_progress": 5
    },
    "calendar_sync": {
      "enabled": true,
      "calendar_id": "primary",
      "reminder_days": [1, 3]
    },
    "auto_assignment": {
      "enabled": true,
      "assign_to_creator": false,
      "default_assignees": []
    },
    "repository_filters": {
      "exclude_repos": [".github", "wf"],
      "include_private": true,
      "include_public": true
    }
  }
}